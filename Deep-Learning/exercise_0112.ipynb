    "from sklearn.model_selection import KFold\n",
    "import numpy as np\n",
    "\n",
    "X = np.array([1, 2, 3, 4, 5])\n",
    "K = 3\n",
    "kf = KFold(n_splits=K)\n",
    "for train_idx, test_idx in kf.split(X):\n",
    "    print(\"Train : \", train_idx)\n",
    "    print(\"Test : \", test_idx)"
   ]
  }
